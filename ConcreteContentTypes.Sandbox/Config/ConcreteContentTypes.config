<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="ConcreteContentTypesSettings" type="ConcreteContentTypes.Core.Configuration.Settings, ConcreteContentTypes.Core" allowLocation="true" allowDefinition="Everywhere"/>
		<section name="ConcreteContentTypeResolvers" type="ConcreteContentTypes.Core.Configuration.TypeResolverSettings, ConcreteContentTypes.Core" allowLocation="true" allowDefinition="Everywhere"/>
	</configSections>
	<ConcreteContentTypesSettings
			Enabled="true"
			GenerateOnContentTypeSave="true"
			CSharpOutputFolder="Models"
			Namespace="ConcreteContentTypes.Sandbox.Models"
			CreateAssembly="false"
			AssemblyName="Concrete.dll"
			AssemblyVersion="1.0.0.0"
			AssemblyOutputFolder="bin" />

	<ConcreteContentTypeResolvers>
		<TypeResolvers>
			<TypeResolver type="ConcreteContentTypes.Core.PropertyTypeResolution.BasicTypeResolver, ConcreteContentTypes.Core">
				<SupportedTypes>
					<Type alias="Umbraco.ColorPickerAlias" />
					<Type alias="Umbraco.CheckBoxList" />
					<Type alias="Umbraco.Date" />
					<Type alias="Umbraco.DateTime" />
					<Type alias="Umbraco.DropDown" />
					<Type alias="Umbraco.DropDownMultiple" />
					<Type alias="Umbraco.NoEdit" />
					<Type alias="Umbraco.Integer" />
					<Type alias="Umbraco.TinyMCEv3" />
					<Type alias="Umbraco.TextboxMultiple" />
					<Type alias="Umbraco.Textbox" />
					<Type alias="Umbraco.TrueFalse" />
					<Type alias="Umbraco.Guid" />
					<Type alias="Umbraco.EmailAddress" />
					<Type alias="Umbraco.MarkdownEditor" />
				</SupportedTypes>
			</TypeResolver>
			<TypeResolver type="ConcreteContentTypes.Core.PropertyTypeResolution.ContentPickerTypeResolver, ConcreteContentTypes.Core">
				<SupportedTypes>
					<Type alias="Umbraco.ContentPickerAlias" />
				</SupportedTypes>
			</TypeResolver>
			<TypeResolver type="ConcreteContentTypes.Core.PropertyTypeResolution.MultiNodeTreePickerTypeResolver, ConcreteContentTypes.Core">
				<SupportedTypes>
					<Type alias="Umbraco.MultiNodeTreePicker" />
				</SupportedTypes>
			</TypeResolver>
			<TypeResolver type="ConcreteContentTypes.Core.PropertyTypeResolution.UmbracoGridTypeResolver, ConcreteContentTypes.Core">
				<SupportedTypes>
					<Type alias="Umbraco.Grid" />
				</SupportedTypes>
			</TypeResolver>
		</TypeResolvers>
	</ConcreteContentTypeResolvers>
</configuration>